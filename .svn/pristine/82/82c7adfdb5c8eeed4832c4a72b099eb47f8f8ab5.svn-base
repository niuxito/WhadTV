@CHARSET "UTF-8";

#loterias{
	width: 300px;
	height: 500px;
	display: block;
	//background-color: green;
	border: 1px red solid;
	border-radius: 5px;	
}

#loterias #msg{
	width: 100%;
	height: 35px;
	display: block;
	float: left;
	padding:8px 0 0 0 ;
	background-color: red;
	color: white;
}

#loterias #mnu{
	width: 100%;
	//height: 30px;
	display: block;
	float: left;
	font-size: 12px;
	//background-color: blue;
}

#loterias #datos{
	width: 100%;
	height: 68%;
	display: block;
	float: left;
//	background-color: blue;
}

body{
	font-size: 14px;
}

#loterias #mnu ul {
	
		
}
#loterias #mnu ul li {
	display: inline;
	padding:4px;
		
}
table{
	border: 1px;
}

[name=numero]{
	width: 40px;
}
[type=button]{
	width: 100px;
	margin: 10px;
}

#loterias img{
	width: 300px;
}
#loterias #nota{
	width: 100%;
	height: 20px;
	display: block;
	float: left;
//	background-color: blue;
}

#loterias .titulo.premio{
	font-weight: bold;
}
#loterias .titulo{
	background-color: red;
	text-align: center;
	clear: both;
	

}
div{
	display: block;
}

#loterias .numero{
	text-align: center;	
	float: left;
	width: 25%;
}
#loterias .numero.premio{
	width:100%;
}
#loterias .sub{
	margin: 1px;
	width: 48%;
	//border-style: solid;
	//border-color: red;
	border: solid 1px red;
	display: block;
	float: left;
	height: 150px;
	
}

#loterias .sub .numero{
	width: 50%;
}
#loterias .numero.reintegro{
	width: 33%;
}
