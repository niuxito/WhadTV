@CHARSET "UTF-8";

body{
	overflow:hidden;
	width: 988px;
	margin: auto;	
}	
.videos .btnadd{
	
	padding: 10px;
	border-radius: 5px;
	width: 70px;
	box-shadow: 0px 1px 1px black;
	float: left;
	font-size: 60%;
}
.videos a{
	text-decoration: none;
	text-align:center;
	display:block;
}
#videos{
	width: 100%;
	clear: both;
	float: left;
	height: 80%;
}

.videos .paginfo{
	float:left;	
}
.videos{
	float:left !important;
	width:75% !important;
	height:85%;
	overflow:hidden;
}

#video{
	//width: 150px;
	//height: 200px;
	margin: 17px;
	float: left;
	box-shadow: 1px 3px 20px black;
	padding:10px;
}
#video img{
	width: 100px;
	height: 100px;
	margin: 10px 10px 5px 25px;
}

#video a{
	text-decoration:none;	
}
div#content{
	height:660px;
	padding:0;	
}
div.listas{
	clear: none;
	width: 200px;
	overflow: scroll;
	height: 100%;
	float:left;	
}

.listas li{
	list-style: none;
	width: 80%;
	//border-radius: 5px;
	background-color: 
	#AAA;
	text-align: center;
	padding: 6px 16px;
	box-shadow: 0px 1px 1px #090707;	
	margin: 5px;
	height: 25px;
}
.listas ul{
	margin:0px	
}
.listas a{
	margin: 5px;
	width: 70%;
	text-align: center;
	text-decoration: none;
	padding: 0 5px;
	display: block;
	float:left;
	
}

.soltador{
	//width:40px !important;	
