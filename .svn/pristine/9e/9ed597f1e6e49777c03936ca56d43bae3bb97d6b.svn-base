html, body {}
body {margin: 0; padding: 0; background: #f0f0f0}
body, table, input, select, textarea {font-family: Arial, Helvetica, sans-serif}
body, table {font-size: 12px; line-height: 18px; color: #444}

#contnr {position: relative; width: 100%; overflow: hidden}

.wrap {position: relative; display: block; width: 100%; padding: 0; margin: 0 auto;}
.sub_wrap {position: relative; display: block; width: 100%}
.wrap, .sub_wrap, .brd_bx, .inpt, h1, h2 {-webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box}


a {text-decoration: none; color: #b74f19} a:hover {color: #016893} a:focus {outline: none}

ol, ul, li {margin: 0; padding: 0; outline-style: none} li {list-style: none}

.clear {clear: both}

p.top, h1.top, h2.top, h3.top {margin-top: 0 !important; padding-top: 0 !important}
p.btm, h1.btm, h2.btm, h3.btm {margin-bottom: 0 !important; padding-bottom: 0 !important}




