/*

	## Formularis ##

*/

.forms form {position: relative; display: inline-block; overflow: hidden; width: 100%}


 .box_error {
	border: 1px solid #d53535;  background: #ffaa5e url(../img/back_form_error_2.gif);
	
}
 .box_error h2 {
	margin: 0 0 5px 0; color: #981c04; font-weight: normal; text-shadow: 1px 1px 0 #ffede7; font-size: 20px; line-height: 28px;
	background: url(../img/icons/ico_alert.png) 0 center no-repeat; padding-left: 35px
}
 .box_error p {font-size: 13px; line-height: 18px; margin: 0 0 10px 0; color: #220600}
 .box_error p.last {margin-bottom: 0}


 .box_info {border: 1px solid #9abbca; background: #d2eaf5 }
 .box_info h2 {margin: 0 0 5px 0; color: #444; font-weight: normal; text-shadow: 1px 1px 0 #fff; font-size: 20px; line-height: 28px}
 .box_info p {font-size: 13px; line-height: 18px; margin: 0 0 10px 0; color: #333}
 .box_info p.last {margin-bottom: 0}

 .box_error, .box_info {
	position: relaitve; display: block; padding: 10px; margin: 10px 0 0 0;
	-moz-border-radius: 5px; border-radius: 5px; -webkit-border-radius: 5px;
}


.forms label {position: relative; display: inline-block; overflow: hidden; ;width: 100%; margin: 10px 0 0 0}
.forms label.ini {}
.forms label.left, .forms label.centr {float: left; width: auto; margin-right: 20px}
.forms label.centr {margin-right: 0}
.forms label.break {clear: right} .forms label.block {float: none; display: block}
.forms label.right {float: right; width: auto; clear: right}
.forms label.line  {width: auto; margin-right: 10px; white-space: nowrap}

.forms label.mdm {width: 50%}
.forms label.nbl {width: 25%}
.forms label.min {width: 150px}

.forms label h3, .forms .title, .colores_box  h3{
	margin: 0; padding: 0 0 5px 0; color: #444; font-weight: normal; font-size: 12px; line-height: 18px; text-shadow: 1px 1px 0 #fff
}
.forms label.line h3 .forms label.line span.title {display: inline-block; padding: 3px}
.forms label h3 .obl {display: inline-block; margin-left: 5px; color: #ff0000}

.forms .inpt {
	position: relative; display: block;
	width: 100%; padding: 5px 10px; background: #fff; font-size: 12px; line-height: 18px; color: #444;
	border: 1px solid #bbb; -moz-border-radius: 3px; border-radius: 3px; -webkit-border-radius: 3px;
}
.forms .fmdm .inpt {width: 50%}
.forms .fmin .inpt {width: 100px}
.forms .fmdm #VideoTiempo, .forms .fmdm #VideoTime{ float: left; }
.forms .fmdm #rangevalue{ float: left; clear: none; margin: 3px 5px; color: #D76323;}

.forms select.inpt, .forms label.left .inpt, .forms label.centr .inpt, .forms label.right .inpt {width: auto !important; min-width: 100px}

.forms .inpt.mdm {width: 50%}
.forms .inpt.nbl {width: 25%}
.forms .inpt.min {width: 150px}

.forms label.mdm .inpt, .forms label.nbl .inpt {width: 100% !important}

.forms label.clndr .inpt {
	padding-left: 22px; background: #fff url(../img/icons/ico_timer_mn.png) 5px center no-repeat; width: 150px !important
}

.forms label.alert h3, .forms label.alert .title {color: #d53535; font-weight: bold}
.forms label.alert .inpt {border-color: #d53535}

.forms label .inpt:focus {border-color: #ffbd00}

.forms label span.inf {
	position: relative; display: block; float: left; clear: both; margin: 5px 0 2px 0; padding: 3px 5px;
	border: 1px solid #ddd; background: #f9f9f9; -moz-border-radius: 5px; border-radius: 5px; -webkit-border-radius: 5px;
	font-size: 11px; line-height: 16px
}
.forms label span.inf a {font-weight: bold}
.forms label span.inf .del {line-height: 0}
.forms label span.inf .del img {
	margin-left: 5px; width: 14px; height: 10px; vertical-align: middle; background: url(../img/icons/ico_del_tag.png)
}

.forms label.line input, .forms label.line select {display: inline-block}

.forms .msg {
	position: relative; float: left; margin-top: 25px; font-size: 11px; line-height: 18px; color: #666;
	background: url(../img/form/back_msg_left.png) 0 0 no-repeat; padding-left: 15px
}
.forms .msg.blk {display: block; float: none}
.forms .msg.btm {background: url(../img/form/back_msg_btm.png) 0 0 no-repeat; padding: 10px 0 0 0; margin: 5px 0 0}
.forms .msg.mdl {width: 50%}
.forms .msg.min {width: 25%}


.forms.sep {padding-bottom: 15px; background: url(../img/form/back_sep.png) 0 bottom repeat-x; margin-bottom: 20px}
.forms .sep {position: relative; overflow: hidden; width: 100%; height: 10px; background: url(../img/form/back_sep.png) 0 bottom repeat-x; margin: 20px 0}

.forms .gnr_inf {position: absolute; left: 0; bottom: 25px; font-size: 10px; line-height: 16px; color: #888; text-shadow: 1px 1px 0 #fff}
.forms .gnr_inf b {color: #d53535}


.forms .box_btns {
	position: relative; display: inline-block; overflow: hidden; width: 100%;
	margin-top: 15px; border-top: 1px dotted #ccc; padding: 15px 0 5px
}

