.list .elm a.prv u, .list .elm a.dsp u, .list .elm a.lbr u, .list .elm a.lst u, .vlist .elm a.prv u, .vlist .elm b.prv u {
	display: inline-block; background: #999; color: #fff; text-decoration: none; text-shadow: -1px -1px 0 #777;
