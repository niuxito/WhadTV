	.msg_inf {position: relative; overflow: hidden; margin: 0 auto; padding: 30px 0; color: #666}
	.msg_inf h1, .msg_inf h2 {font-size: 22px; line-height: 28px; text-shadow: 1px 1px 0 #fff; margin: 0 0 15px}
	.msg_inf p {margin: 0; padding: 0 0 15px 0}
	.msg_inf, .msg_inf p {font-size: 16px; line-height: 22px}




