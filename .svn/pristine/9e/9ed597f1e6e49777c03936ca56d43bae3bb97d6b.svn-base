	border: 1px solid #444; background: #555; -moz-border-radius: 4px; border-radius: 4px; -webkit-border-radius: 4px;
	-moz-box-shadow: 0 0 5px #aaa; -webkit-box-shadow: 0 0 5px #aaa; box-shadow: 0 0 5px #aaa; vertical-align: middle
}
a.lbtn {float: none; display: inline-block; margin: 4px 10px 4px 0}
.forms .btn.up, a.lbtn.up, .box_btns .btn.up, .box_ops h1 a.btn.up, .mlist .ops .btn.up {
	font-size: 16px; line-height: 16px; background: #d76323; border-color: #c55216; text-shadow: -1px -1px 0 #bd4807;
	margin-top: 0; padding: 7px 15px;
}


.box_btns .btn.up, .box_ops h1 a.btn.up {padding: 7px 10px}
.forms .btn.dwn, a.lbtn.dwn {
	padding: 3px 5px; background: #999; border-color: #888; text-shadow: -1px -1px 0 #666;
}
.forms .btn.st_updt, .box_btns .btn.st_updt, a.lbtn.st_updt {
	background: #555 url(../img/icons/ico_refresh_w_m.png) 5px center no-repeat; padding-left: 25px;
}
.forms .btn.up.st_updt, .box_btns .btn.up.st_updt, a.lbtn.up.st_updt {
	background: #d76323 url(../img/icons/ico_refresh_w.png) 5px center no-repeat; padding-left: 35px;
}
.forms .btn:hover, .box_btns .btn:hover, a.lbtn:hover, .box_ops h1 a.btn.up:hover {
	background-color: #e96521; border: 1px solid #c55216;
	-moz-text-shadow: -1px -1px 0 #cf4f0d; text-shadow: -1px -1px 0 #cf4f0d; -webkit-text-shadow: -1px -1px 0 #cf4f0d;
	-moz-box-shadow: 0 0 5px #ff7e00; -webkit-box-shadow: 0 0 5px #ff7e00; box-shadow: 0 0 5px #ff7e00;
}

 .mlist .ops .btn.up{
 	float:left;
 	padding-top: 3px;
 	padding-bottom: 3px;
 }





