	/* menu usuari */

	#menu {
		position: absolute; overflow: hidden; right: 20px; top: 0; height: 30px; white-space: nowrap; text-align: right;
		-moz-border-radius-bottomleft: 5px; border-bottom-left-radius: 5px; -webkit-border-bottom-left-radius: 5px;
